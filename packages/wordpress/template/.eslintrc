{
  "ignorePatterns": [
    "node_modules",
    "public"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "html",
    "vue"
  ],
  "extends": [
    "standard",
    "plugin:vue/recommended"
  ],
  "rules": {
    "vue/html-self-closing": "off",
    "vue/max-attributes-per-line": "off"
  }
}
